{
  "name": "vue-meteor-demo",
  "private": true,
  "scripts": {
    "start-server": "cross-env METEOR_LOCAL_DIR=.meteor/test meteor run --settings settings.json --inspect",
    "@comment start-server": "cross-env MONGO_URL='mongodb://127.0.0.1:27017/commonmembership_test' meteor run --settings settings.json  --port 3052",
    "cypress:gui": "cypress open",
    "cypress:headless": "cypress run --headless --browser chrome",
    "test:gui": "start-server-and-test start-server http://localhost:3000 cypress:gui",
    "test:headless": "start-server-and-test start-server http://localhost:3000 cypress:headless",
    "@comment start": "remove the inspect property below if you do not need to debug application",
    "start": "cross-env MONGO_URL='mongodb://127.0.0.1:27017/commonmembership_dev' meteor run --settings settings.json",
    "debugnodejs": "meteor run --settings settings.json --inspect",
    "debug": "meteor run --settings settings.json --inspect-brk=19229",
    "unittest": "cross-env TEST_WATCH=1 MONGO_URL='mongodb://127.0.0.1:27017/commonmembership_test' meteor test --settings settings-test.json --once --driver-package meteortesting:mocha",
    "test-app": "cross-env TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 3051",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@riophae/vue-treeselect": "^0.4.0",
    "aws-sdk": "^2.556.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "core-js": "^3.3.2",
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "intersection-observer": "^0.6.0",
    "isomorphic-fetch": "^2.2.1",
    "marker-clusterer-plus": "^2.1.4",
    "meteor-node-stubs": "^1.0.0",
    "moment": "^2.24.0",
    "pug": "^2.0.4",
    "quasar": "^1.4.5",
    "simpl-schema": "^1.5.6",
    "vue": "^2.6.10",
    "vue-googlemaps": "^0.1.2",
    "vue-meteor-tracker": "^2.0.0-beta.5",
    "vue-observe-visibility": "^0.4.6",
    "vue-router": "^3.1.3",
    "vue-supply": "^0.3.0",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0",
    "winston": "^3.2.1",
    "winston-loggly-bulk": "^3.0.1"
  },
  "devDependencies": {
    "@types/meteor": "^1.4.37",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "@types/underscore": "^1.9.4",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "chai": "^4.2.0",
    "cypress": "^3.8.0",
    "meteor-typings": "^1.4.1",
    "sinon": "^7.5.0",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "vue-template-compiler": "2.6.10"
  }
}
